# Root BUILD for convenient aliases
package(default_visibility = ["//visibility:public"])

alias(
    name = "main_cpp",
    actual = "//main:main_cpp",
)

alias(
    name = "main_py",
    actual = "//main:main_py",
)

alias(
    name = "helper_lib",
    actual = "//main:helper_lib",
)

alias(
    name = "hello_script",
    actual = "//scripts:hello_script",
)
